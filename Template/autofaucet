function claim($coin,$number){
	$r = curl(host."auto/currency/".$coin,h());check();
	if(preg_match("/You don't have enough energy for Auto Faucet!/",$r)){
		exit(m."You don't have enough energy for Auto Faucet!\n");
	}
	$tmr = explode(',',explode('let timer = ',$r)[1])[0];
	if($tmr){tmr($tmr);}
	
	$token = explode('">',explode('<input type="hidden" name="token" value="',$r)[1])[0];
	if(!$token)return 0;
	$data = "token=".$token;
	$r = curl(host."auto/verify/".$coin,h(),$data);
	$ss = explode("account!'",explode("html: '0.",$r)[1])[0];
	$wr = explode("'",explode("html: '",$r)[1])[0];
	$ban = explode('</div>',explode('<div class="alert text-center alert-danger"><i class="fas fa-exclamation-circle"></i> Your account',$r)[1])[0];
	if($ban){
		print "\r                      \r";
		exit(m."Your account".$ban.n);
	}
	if($ss){
		print h."0.".$ss.n;
	}elseif($wr){
		print c.$coin.p.": ".m.$wr;
		$stop = explode('sufficient funds',$wr)[1];
		if($stop){
			print n;
			return [$number => "sufficient funds"];
		}
		sleep(3);
		print "\r                  \r";
	}else{
		print k."Gagal Claim";
		sleep(3);
		print "\r                  \r";
	}
	return [$number => "mantap"];
}
function config(){
	simpan("Cookie");
	simpan("User_Agent");
}
function h($data=0){
	$h[] = "user-agent: ".simpan("User_Agent");
	$h[] = "cookie: ".simpan("Cookie");
	return $h;
}

